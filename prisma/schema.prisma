generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Deck {
  authorId    Int?
  description String?
  id          Int     @default(autoincrement()) @id
  published   Boolean @default(false)
  title       String
  User        User?   @relation(fields: [authorId], references: [id])

  @@index([authorId], name: "authorId")
}

model User {
  email String  @unique
  id    Int     @default(autoincrement()) @id
  name  String?
  Deck  Deck[]
}