// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Deck {
  id Int @default(autoincrement()) @id
  name String
  description String
}

model Card {
  id Int @default(autoincrement()) @id
  question String
  answer String
  score Int
}